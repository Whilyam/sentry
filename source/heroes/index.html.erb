<div class="row">
  <div class="col-md-9">
    <% panel do %>
      <div class="panel-body">
        <% sitemap.where(:layout.equal => 'hero', :index.equal => true).order_by(:hero).all().each do |page| %>
          <a href="<%=  page.url %>">
            <img src="<%= img_src "heroes", page.data.hero %>" alt="<%= page.data.hero %>" width="100">
          </a>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
